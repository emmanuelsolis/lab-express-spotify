<table class="uk-table uk-table-hover uk-table-divider">
  <thead>
    <tr>
      <th class="uk-heading-medium uk-secondary uk-text-bold uk-text-warning">Title</th>
      <th class="uk-heading-medium uk-secondary uk-text-bold uk-text-warning">Listen</th>

    </tr>
  </thead>
  <tbody>
    {{#each track}}
      <tr>
        <td>{{name}}</td>
        <td>
          {{#if preview_url}}
            <audio controls src={{preview_url}}>
              <code>audio</code>.
            </audio>
          {{else}}
          No hay preview
          {{/if}}
        </td>
      </tr>
    {{/each}}

  </tbody>
</table>